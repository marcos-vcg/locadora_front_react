name: Development GitHub Pages

on:
  push: 
    branches: ["*"]
    
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v3
      - uses: actions/setup-node@2
        with:
          node-version: 16
      - run: npm install
      - run: npm run test
      - run: npm run build
      - run: npm run deploy
